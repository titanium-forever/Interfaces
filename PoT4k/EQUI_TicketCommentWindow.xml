<?xml version="1.0" encoding="us-ascii"?>
<XML ID="EQInterfaceDefinitionLanguage">
  <Schema xmlns="EverQuestData" xmlns:dt="EverQuestDataTypes" />
  <!-- *********** Ticket Comment Window *********** -->
  <Editbox item="TCW_CommentBox">
    <ScreenID>TCW_CommentBox</ScreenID>
    <Font>5</Font>
    <DrawTemplate>WDT_Inner</DrawTemplate>
    <RelativePosition>true</RelativePosition>
    <Style_Border>true</Style_Border>
    <TextColor>
      <R>255</R>
      <G>255</G>
      <B>255</B>
    </TextColor>
    <AutoStretch>true</AutoStretch>
    <LeftAnchorOffset>10</LeftAnchorOffset>
    <TopAnchorOffset>10</TopAnchorOffset>
    <RightAnchorOffset>10</RightAnchorOffset>
    <BottomAnchorOffset>70</BottomAnchorOffset>
    <TopAnchorToTop>true</TopAnchorToTop>
    <BottomAnchorToTop>false</BottomAnchorToTop>
    <RightAnchorToLeft>false</RightAnchorToLeft>
    <LeftAnchorToLeft>true</LeftAnchorToLeft>
    <Style_Transparent>false</Style_Transparent>
    <Style_VScroll>true</Style_VScroll>
    <Style_Multiline>true</Style_Multiline>
  </Editbox>
  <Button item="TCW_SubmitButton">
    <ScreenID>TCW_SubmitButton</ScreenID>
    <Font>6</Font>
    <RelativePosition>true</RelativePosition>
    <AutoStretch>true</AutoStretch>
    <LeftAnchorOffset>10</LeftAnchorOffset>
    <TopAnchorOffset>50</TopAnchorOffset>
    <RightAnchorOffset>210</RightAnchorOffset>
    <BottomAnchorOffset>10</BottomAnchorOffset>
    <TopAnchorToTop>false</TopAnchorToTop>
    <BottomAnchorToTop>false</BottomAnchorToTop>
    <RightAnchorToLeft>true</RightAnchorToLeft>
    <LeftAnchorToLeft>true</LeftAnchorToLeft>
    <Style_VScroll>false</Style_VScroll>
    <Style_HScroll>false</Style_HScroll>
    <Style_Transparent>false</Style_Transparent>
    <Style_Checkbox>false</Style_Checkbox>
    <Text>Submit</Text>
    <TooltipReference>Submit this comment to be appended to the ticket</TooltipReference>
    <TextColor>
      <R>255</R>
      <G>255</G>
      <B>255</B>
    </TextColor>
    <ButtonDrawTemplate>
      <Normal>A_BtnNormal</Normal>
      <Pressed>A_BtnPressed</Pressed>
      <Flyby>A_BtnFlyby</Flyby>
      <Disabled>A_BtnDisabled</Disabled>
      <PressedFlyby>A_BtnPressedFlyby</PressedFlyby>
    </ButtonDrawTemplate>
  </Button>
  <Button item="TCW_CancelButton">
    <ScreenID>TCW_CancelButton</ScreenID>
    <Font>6</Font>
    <RelativePosition>true</RelativePosition>
    <AutoStretch>true</AutoStretch>
    <LeftAnchorOffset>210</LeftAnchorOffset>
    <TopAnchorOffset>50</TopAnchorOffset>
    <RightAnchorOffset>10</RightAnchorOffset>
    <BottomAnchorOffset>10</BottomAnchorOffset>
    <TopAnchorToTop>false</TopAnchorToTop>
    <BottomAnchorToTop>false</BottomAnchorToTop>
    <RightAnchorToLeft>false</RightAnchorToLeft>
    <LeftAnchorToLeft>false</LeftAnchorToLeft>
    <Style_VScroll>false</Style_VScroll>
    <Style_HScroll>false</Style_HScroll>
    <Style_Transparent>false</Style_Transparent>
    <Style_Checkbox>false</Style_Checkbox>
    <Text>Cancel</Text>
    <TooltipReference>Cancel the comment and close the window</TooltipReference>
    <TextColor>
      <R>255</R>
      <G>255</G>
      <B>255</B>
    </TextColor>
    <ButtonDrawTemplate>
      <Normal>A_BtnNormal</Normal>
      <Pressed>A_BtnPressed</Pressed>
      <Flyby>A_BtnFlyby</Flyby>
      <Disabled>A_BtnDisabled</Disabled>
      <PressedFlyby>A_BtnPressedFlyby</PressedFlyby>
    </ButtonDrawTemplate>
  </Button>
  <Screen item="TicketCommentWindow">
    <ScreenID />
    <Font>6</Font>
    <RelativePosition>false</RelativePosition>
    <Location>
      <X>80</X>
      <Y>80</Y>
    </Location>
    <Size>
      <CX>600</CX>
      <CY>400</CY>
    </Size>
    <Text>Append Ticket Comment</Text>
    <Style_VScroll>false</Style_VScroll>
    <Style_HScroll>false</Style_HScroll>
    <Style_Transparent>false</Style_Transparent>
    <TooltipReference />
    <DrawTemplate>WDT_Def</DrawTemplate>
    <Style_Titlebar>true</Style_Titlebar>
    <Style_Closebox>true</Style_Closebox>
    <Style_Minimizebox>true</Style_Minimizebox>
    <Style_Border>true</Style_Border>
    <Style_Sizable>true</Style_Sizable>
    <Pieces>TCW_CommentBox</Pieces>
    <Pieces>TCW_SubmitButton</Pieces>
    <Pieces>TCW_CancelButton</Pieces>
  </Screen>
</XML>